{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "forOf": true,
    "modules": [2],
    "superInFunctions": true,
    "templateStrings": true
  },
  "rules": {
    "no-dupe-args": true,
    "no-dupe-keys": true,
    "no-unussed-vars": true,
    "no-multi-spaces: false,
    "no-use-before-define": "nofunc",
    "eqeqeq": [2, "smart"],
    "block-scoped-var": [0],
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "camelcase": [0],
    "quotes": "single"
  }
}
